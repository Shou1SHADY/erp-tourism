{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "vite build",
    "devCommand": "vite",
    "installCommand": "npm install",
    "framework": "vite",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}